import pandas as pd

# Create a dictionary of data
data = {
    'Name': ['Alice', 'Bob', 'Charlie', 'David'],
    'Age': [25, 30, 22, 28],
    'City': ['New York', 'London', 'Paris', 'Tokyo']
}

# Create a DataFrame from the dictionary
df = pd.DataFrame(data)

print(df)
# Select a column
names = df['Name']
print("\nSelected column 'Name':")
print(names)

